{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_website}",
    "sources": "${mod_repository}"
  },
  "license": "${mod_license}",
  "icon": "assets/tinymobfarm/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.daqem.tinymobfarm.fabric.TinyMobFarmCommonFabric"
    ],
    "client": [
      "com.daqem.tinymobfarm.fabric.TinyMobFarmClientFabric"
    ]
  },
  "mixins": [
    "tinymobfarm.mixins.json",
    "tinymobfarm-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "supermartijn642configlib": "${config_library_version}",
    "uilib": "${ui_library_version}"
  }
}